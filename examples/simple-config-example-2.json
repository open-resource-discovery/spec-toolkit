{
  "$schema": "https://open-resource-discovery.github.io/spec-toolkit/spec-v1/spec-toolkit-config.schema.json#",
  "generalConfig": {
    "sortProperties": false
  },
  "outputPath": "src/generated/spec-v1",
  "docsConfig": [
    {
      "type": "spec",
      "id": "my-spec",
      "sourceFilePath": "./spec/v1/my-spec.schema.yaml",
      "sourceIntroFilePath": "./spec/v1/my-spec-intro.schema.md",
      "sourceOutroFilePath": "./spec/v1/my-spec-outro.schema.md",
      "examplesFolderPath": "./spec/v1/examples",
      "mdFrontmatter": {
        "title": "Interface Documentation for my-spec",
        "sidebar_position": "1",
        "description": "Describes the technical interface / schema for my-spec.",
        "toc_max_heading_level": "4"
      }
    },
    {
      "type": "specExtension",
      "id": "my-spec-extension-1",
      "sourceFilePath": "./spec/v1/my-spec-extension-1.yaml",
      "sourceIntroFilePath": "./spec/v1/my-spec-extension-1.md",
      "targetDocumentId": "my-spec",
      "mdFrontmatter": {
        "title": "Extension 1",
        "sidebar_position": "2",
        "description": "Extension 1 description."
      }
    },
    {
      "type": "specExtension",
      "id": "my-spec-extension-2",
      "sourceFilePath": "./spec/v1/my-spec-extension-2.yaml",
      "sourceIntroFilePath": "./spec/v1/my-spec-extension-2.md",
      "targetDocumentId": "my-spec",
      "mdFrontmatter": {
        "title": "Extension 2",
        "sidebar_position": "3",
        "description": "Extension 2 description."
      }
    }
  ]
}
